GRAB 300
LINK 800
COPY 0 #CLRS
COPY 0 X

MARK LOOP1
COPY 0 #DATA
COPY X #DATA
COPY F #DATA
ADDI X 1 X
TEST X = 9
TJMP LOOP2
JUMP LOOP1

MARK LOOP2
COPY 0 X
MARK LOOP2S
COPY 1 #DATA
COPY X #DATA
COPY F #DATA
ADDI X 1 X
TEST X = 9
TJMP LOOP3
JUMP LOOP2S

MARK LOOP3
COPY 0 X
MARK LOOP3S
COPY 2 #DATA
COPY X #DATA
COPY F #DATA
ADDI X 1 X
TEST X = 9
FJMP LOOP3S
LINK -1
HALT